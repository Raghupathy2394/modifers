	//static vs public
	public static void mystaticmethod() {
		System.out.println("helo");
	}
	public static void mypublicmethod() {
		System.out.println("world");
	}
	public static void main(String[] args) {
		mystaticmethod();
		Main mypublicmethod=new Main();
		
	}
	}