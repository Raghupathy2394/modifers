// if  else//
	static void checkAge(int age) {
	if(age < 18)
	{ System.out.println("not eligible to vote");
	}
	else{
       System.out.println("eligible to vote");		
	}
	}
	public static void main(String[] args) {
	checkAge(20);
	}
	
