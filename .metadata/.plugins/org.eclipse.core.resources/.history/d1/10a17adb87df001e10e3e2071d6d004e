///multiple parameters//
static void myMethods(String fname,int age) {
	System.out.println(fname +"is" + age);
	}
public static void main(String[] args) {
    myMethods("raj",30);	
    myMethods("ravi",31);
    myMethods("rahul",32);
	}
}